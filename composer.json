    "description": "ApexCharts builder for Laravel",
    "authors" : [{
        "name" : "DragonOfMercy",
        "email" : "dragonofmercy@gmail.com",
        "role" : "Developer"
    }],
    "homepage" : "https://github.com/dragonofmercy/laravel-apexcharts",
